{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "import/no-unresolved": [2, { ignore: ['\.jpg$', '\.png$'] }],
        "indent": [2, 4],
        "no-use-before-define": 0,
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-no-bind": [2, { "ignoreRefs": true }]
    }
}
