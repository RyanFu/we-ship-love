{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "import/no-unresolved": [2, { ignore: ['\.jpg$', '\.png$'] }],
        "react/forbid-props-types": "off",
        "react/jsx-filename-extension": "off",
        "react/jsx-no-bind": ["error", { "ignoreRefs": true }]
    }
}
